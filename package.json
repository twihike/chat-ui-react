{
  "name": "chat-ui-react",
  "version": "0.2.3",
  "description": "React component for conversational UI.",
  "license": "MIT",
  "author": "twihike",
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "browser": "dist/browser/chat-ui-react.umd.min.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/twihike/chat-ui-react.git"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:clean": "rimraf dist",
    "build:types": "tsc -p tsconfig.build.json",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel src --out-dir dist/esm --extensions \".ts,.tsx,js,jsx\"",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel src --out-dir dist/cjs --extensions \".ts,.tsx,js,jsx\"",
    "build:browser": "cross-env NODE_ENV=production rollup -c",
    "lint": "eslint --ext .js,.ts,.tsx src",
    "format": "eslint --fix --ext .js,.ts,.tsx src"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.9.2",
    "@material-ui/icons": "^4.9.1",
    "audio-recorder-polyfill": "^0.3.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@material-ui/core": "^4.9.13",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "@rollup/plugin-typescript": "^4.1.1",
    "@twihike/eslint-config": "^0.1.5",
    "@twihike/node-toolchain": "^0.1.5",
    "@types/dom-mediacapture-record": "^1.0.5",
    "@types/node": "^13.13.5",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "rollup": "^2.8.0",
    "rollup-plugin-filesize": "^8.0.2",
    "rollup-plugin-license": "^2.0.0",
    "rollup-plugin-node-license": "^0.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-sizes": "^1.0.2",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-visualizer": "^4.0.4",
    "standard-version": "^8.0.0"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.9.13",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "optionalDependencies": {}
}
